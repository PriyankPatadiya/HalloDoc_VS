<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.11.3/font/bootstrap-icons.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    
    <link rel="stylesheet" href="~/HalloDoc.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/PatientDashboard.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/AdminDashboardMain.css" />

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" />
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>

    
    <script src="~/js/AdminDashboard.js" asp-append-version="true"></script> 


    <title>@ViewData["Title"]</title>
</head>
<body>
    <header class="">
        <nav class="navbar navbar-expand-lg bg-white d-flex justify-content-between border border-bottom">
            <img src="~/images/hallodoc2.png"
                 alt=""
                 style="height: 8vh; margin-left: 4vw"
                 class=" ms-sm-3 ms-5 ps-3" />

            <div class="text-center align-items-center">
                <span class="text-secondary d-none d-sm-inline"> Welcome </span>
                <span class="fw-bold fs-6 text-black mt-1">@ViewBag.username</span>
                <a asp-controller="Home" asp-action="Logout"> <button class="btn text-decoration-none border border-info bg-white text-info shadow-none mx-2">Logout</button></a>
                <button class="btn bg-transparent pt-1 pb-2 border border-info text-info me-4 shadow-none"
                        id="themebtn" onclick="toggletheme()">
                    <svg xmlns="http://www.w3.org/2000/svg"
                         width="16"
                         height="16"
                         fill="currentColor"
                         class="bi bi-moon "
                         viewBox="0 0 16 16">
                        <path d="M6 .278a.77.77 0 0 1 .08.858 7.2 7.2 0 0 0-.878 3.46c0 4.021 3.278 7.277 7.318 7.277q.792-.001 1.533-.16a.79.79 0 0 1 .81.316.73.73 0 0 1-.031.893A8.35 8.35 0 0 1 8.344 16C3.734 16 0 12.286 0 7.71 0 4.266 2.114 1.312 5.124.06A.75.75 0 0 1 6 .278M4.858 1.311A7.27 7.27 0 0 0 1.025 7.71c0 4.02 3.279 7.276 7.319 7.276a7.32 7.32 0 0 0 5.205-2.162q-.506.063-1.029.063c-4.61 0-8.343-3.714-8.343-8.29 0-1.167.242-2.278.681-3.286" />
                    </svg>
                </button>

            </div>

        </nav>


    </header>
    <div class="w-100 shadow ">
        <nav class="navbar navbar-expand-lg py-0 shadow">
            <div class="container-fluid">
                <button type="button" id="menuadmin" class="navbar-toggler position-absolute start-0 top-0 shadow-none btn btn-outline-info ms-3 mt-4" data-bs-toggle="collapse" data-bs-target="#navbarTogglerDemo01" aria-controls="navbarTogglerDemo01" aria-expanded="false" aria-label="Toggle navigation">
                    <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-list" viewBox="0 0 16 16">
                        <path fill-rule="evenodd" d="M2.5 12a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5m0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5m0-4a.5.5 0 0 1 .5-.5h10a.5.5 0 0 1 0 1H3a.5.5 0 0 1-.5-.5" />
                    </svg>
                </button>
                <div class="collapse navbar-collapse" id="navbarTogglerDemo01">

                    <ul class="nav nav-underline " id="pills-tab" role="tablist">
                        <li class="nav-item " role="presentation">
                            <a class="px-2 nav-link text-decoration-none active " asp-action="MainPage">Dashboard</a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="px-2 nav-link text-decoration-none" id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">Provider Location</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="px-2 nav-link text-decoration-none " id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">MyProfile</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="px-2 nav-link text-decoration-none " id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">Providers</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="px-2 nav-link text-decoration-none" id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">Partners</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="px-2 nav-link text-decoration-none" id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">Access</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="px-2 nav-link text-decoration-none" id="pills-profile-tab" data-bs-toggle="pill" data-bs-target="#pills-profile" type="button" role="tab" aria-controls="pills-profile" aria-selected="false">Records</button>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>


    </div>


    <div class="container-fluid px-0">
        <main role="main" class="">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div style="height: 8vh;" class="bg-dark d-flex flex-row-reverse align-items-center pe-3" id="submitfooter">
            <span class="text-light fw-semibold">Terms of conditions | Privacy Policy</span>
        </div>
    </footer>

    <script>
        $(document).ready(function () {
            $('ViewCaseAdmin').hide();

        });
    </script>
</body>
</html>