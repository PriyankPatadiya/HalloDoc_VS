                                    <path d="M8 8a3 3 0 1 0 0-6 3 3 0 0 0 0 6m2-3a2 2 0 1 1-4 0 2 2 0 0 1 4 0m4 8c0 1-1 1-1 1H3s-1 0-1-1 1-4 6-4 6 3 6 4m-1-.004c-.001-.246-.154-.986-.832-1.664C11.516 10.68 10.289 10 8 10s-3.516.68-4.168 1.332c-.678.678-.83 1.418-.832 1.664z" />
                                </svg>
                            </button>

                            Requestor: @item.RequestorName
                        </h6>

                        <div class="row gy-2  py-3">
                            <div class="col-6 px-2">
                                <a data-bs-toggle="modal" data-bs-target="#AssignCaseModal1_@i"><button class="border-1 text-light rounded-5 btn w-100 " style="background-color: indigo;"> Assign Case</button></a>
                            </div>
                            <div class="col-6 px-2">
                                <a data-bs-toggle="modal" data-bs-target="#staticBackdrop1_@i"><button class=" border-1 btn-danger px-3 rounded-5 btn w-100"> Cancel Case</button></a>

                            </div>
                            <div class="col-6 px-2">
                                <a asp-action="ViewNotesAdmin" asp-route-reqid="@item.reqid"><button class=" border-1 btn-success px-3 rounded-5 btn w-100"> View Notes</button></a>

                            </div>
                            <div class="col-6 px-2">
                                <a data-bs-toggle="modal" data-bs-target="#BlockCaseModal1_@i"><button class=" border-1 btn-danger px-3 rounded-5 btn w-100"> Block Patient</button></a>

                            </div>
                            <div class="col-6 px-2">
                                <a href="mailto: @item.Email"> <button class="border-1 btn-success px-3 rounded-5 btn w-100"> Email</button> </a>

                            </div>
                        </div>

                    </div>
                </div>
            </div>
            i++;
        }
    }

</div>

@*pagination*@

<div class="pb-2 d-flex justify-content-between">
    <nav id="pagination" aria-label="Page navigation" style="width: 80%;">
        <ul class="pagination mb-0">
            @if (ViewBag.CurrentPage >= 1 && ViewBag.CurrentPage <= ViewBag.TotalPages)
            {
                int prevpage = Convert.ToInt32(ViewBag.CurrentPage) - 1;
                int currpage = Convert.ToInt32(ViewBag.CurrentPage);
                int nextpage = Convert.ToInt32(ViewBag.CurrentPage) + 1;
                <li class="page-item">
                    <a class="page-link btn rounded-4 btn-outline-info border border-secondary mx-1" id="previous" data-page="@(prevpage)" aria-label="Previous" @(ViewBag.CurrentPage <= 1 ? "hidden" : "")>
                        &laquo;
                    </a>
                </li>
                        <li class="page-item">
                    <a class="page-link btn rounded-4 btn-outline-info mx-1 border border-secondary" id="current" data-page="@(currpage)" aria-label="Current">
                                @(ViewBag.CurrentPage)
                            </a>
                        </li>
                <li class="page-item">
                    <a class="page-link btn rounded-4 btn-outline-info mx-1 border border-secondary" id="next" data-page="@(nextpage)" aria-label="Next" @(ViewBag.CurrentPage >= ViewBag.TotalPages ? "hidden" : "")>
                        &raquo;
                    </a>
                </li>
            }
        </ul>
    </nav>
    @if(ViewBag.EmptyMessage == null)
    {
        <span>@ViewBag.CurrentPage of @ViewBag.TotalPages</span>
    }
</div>

@*scripts*@


<script>

    function filterPhysicianByRegionn(RegionId) {
        if (RegionId != "0") {
            $.ajax({
                type: "GET",
                url: "/AdminDashboard/filterPhyByRegion",
                data: { RegionId: RegionId },

                success: function (data) {
                    $('#physicianDrop').empty();
                    $.each(data, function (index, item) {
                        $('#physicianDrop').append($('<option>').text(item.firstName).attr('value', item.physicianId));

                    });
                    $('#physicianDrop option:first').prop('selected', true);
                }
            });
        }
    }


    function filterPhysicianByRegion(RegionId, i) {
        if (RegionId != "0") {
            $.ajax({
                type: "GET",
                url: "/AdminDashboard/filterPhyByRegion",
                data: { RegionId: RegionId },

                success: function (data) {

                    $('#physicianDrop1_' + i).empty();
                    $.each(data, function (index, item) {

                        $('#physicianDrop1_' + i).append($('<option>').text(item.firstName).attr('value', item.physicianId));
                    });
                    $('#physicianDrop1_' + i + 'option:first').prop('selected', true);
                }
            });
        }
    }
</script>
@if (ViewBag.EmptyMessage == null)
{
    <script>
        function openAssignModal(requestid) {
            var regions = @Json.Serialize(Model[0].regionTable.ToList());
            regions.forEach(region => {
                $('#Regionnid').append($('<option>').text(region.name).attr('value', region.regionId));
            });
            $('#assignRequestidInput').attr('value', requestid);
            $('#AssignForm').attr('action', '/AdminDashboard/AssignCase?reqid=' + requestid);
            $('#AssignCaseModal').modal('show');
        }
        function openCancelModal(requestid, patientname) {
            $("#cancelModalRequestId").attr('value', requestid);
            $("#patientnamecancelmodal").html(patientname);
            $("#staticBackdrop").modal('show');
        }
        function checkIfValidCancelCase() {
            var notes = $("#CancelNote").val();
            var reasonId = $("#ReasonCancelId").val();
            if (reasonId == 0) {
                alert("Please Select Reason");
            } else if (notes == "") {
                alert("Please Write Notes");
            } else {
                $("#CancelCaseForm").submit();
            }
        }
        function checkAssignCaseForm() {
            var regionId = $("#Regionnid").val();
            var notes = $("#AssignNote").val();
            if (regionId == 0) {
                alert("Please Select Region");
            } else if (notes == "") {
                alert("Please Write Notes");
            } else {
                $("#AssignForm").submit();
            }
        }
    </script>
}