@model DAL.ViewModels.PhysicianProfileVM@{    ViewData["Title"] = "CreateAccount";
    Layout = "~/Views/Shared/_LayoutAdminDash.cshtml";}<style>    .dropdown-menu.columns-3 {        column-count: 3;    }    .column {        display: inline-block;        margin-right: 1rem;        / Adjust as needed /    }    .form-check {        margin-bottom: 0.5rem;        / Adjust as needed /    }</style><link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous"><section>    <div class="container-fluid bg-body-secondary ">        <div class="container bg-body-secondary ">            <div class="d-flex justify-content-between pt-4">                <div class="d-flex align-middle">                    <h4 class="my-0 text-center">Create Provider Profile</h4>                </div>                <div>                    <a asp-action="Provider" class="btn btn-outline-info shadow-none"> ❮  Back </a>                </div>            </div>        </div>        <div class="container border rounded-3 p-3 p-md-5 mb-3 d-flex flex-column bg-body mt-3">            <h5 class="fw-bold my-3">Account Information</h5>            <form method="post" asp-action="ResetPhysicianPassword" id="ResetPhysicianPassword">                <input class="d-none" asp-for="physicianid" />                <div class="row row-cols-1 row-cols-md-2 g-3">                    <div class="col">                        <div class="form-floating">                            <input asp-for=Username type="text" class="form-control" id="username" placeholder="User Name">                            <label for="username">User Name</label>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=Password type="text" class="form-control" id="Password" placeholder="Password" name="Password">                            <label for="Password">Password</label>                            <span class="error-Password text-danger"></span>                        </div>                    </div>                    <div class="col">                        <div class="form-floating mb-3">                            <select name="" class="py-2 form-control mb-3 w-100 " id="">                                <option>Admin</option>                                <option>MasterAdmin</option>                            </select>                        </div>                    </div>                </div>            </form>            <h5 class="fw-bold my-3">Physician Information</h5>            <form asp-action="PhysicianInformation" method="post" asp-route-id="physicianid">                <div class="row row-cols-1 row-cols-md-2 g-3">                    <div class="col">                        <input value="physicianid" class="d-none" asp-for="physicianid" />                        <div class="form-floating">                            <input asp-for=FirstName type="text" class="form-control " id="firstname" placeholder="First Name" name="firstname">                            <label for="firstname">First Name</label>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=LastName type="text" class="form-control" id="lastname" placeholder="Last Name">                            <label for="lastname">Last Name</label>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=Email type="text" class="form-control" id="email" placeholder="Email" name="email">                            <label for="email">Email</label>                            <span class="error-email text-dange"></span>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input id="phone1" type="tel" class="form-control  changable" asp-for="MobileNo" />                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=MedicalLicense type="text" class="form-control changable" id="email" placeholder="Email" name="MedicalLicense">                            <label for="email">Medical License#</label>                            <span class="error-email text-dange"></span>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=NPINumber type="text" class="form-control changable" id="email" placeholder="Email" name="NPINumber">                            <label for="email">NPI Number</label>                            <span class="error-email text-dange"></span>                        </div>                    </div>                    <div class="dropdown col-sm-12 col-md-12 col-lg-6 my-3 form-floating">                        <button class="btn bg-transparent dropdown-toggle form-control py-3 btn-outline-secondary shadow-none" type="button" data-bs-toggle="dropdown" aria-expanded="false">                            Select States                        </button>                        @*<ul class="dropdown-menu w-100 px-4 columns-3" style="height: 450px;">                        @{                        var k = 0;                        var b = 0;                        var totalStates = Model.Regions.Count;                        var statescount = Model.WorkingRegions.Count;                        var statesPerColumn = (int)Math.Ceiling((double)totalStates / 3);                        }                        @for (int i = 0; i < 3; i++)                        {                        <div class="column">                        @for (int j = 0; j < statesPerColumn && k < totalStates; j++)                        {                        <li class="form-check form-check-inline">                        <input class="form-check-input changable" type="checkbox" id="region-Regions[k].RegionId" name="Region" value="Regions[k].RegionId" @(Model.WorkingRegions.Any(wr => wr.RegionId == Model.Regions[k].RegionId) ? "checked" : "")  />                        <label class="form-check-label" for="region-Regions[k].RegionId">Regions[k].Name</label>                        </li>                        k++;                        }                        </div>                        }                        </ul>*@                    </div>                </div>            </form>            <h5 class="fw-bold my-3">Mailing & Billing Information</h5>            <form id="accountinginfo" asp-action="MailingBillingInformationProvider" asp-route-physicianid="physicianid">                <div class="row row-cols-1 row-cols-md-2 g-3">                    <div class="col">                        <div class="form-floating">                            <input asp-for="Address1" type="text" class="form-control changable1" id="address1" placeholder="Address 1" name="Address1">                            <label for="address1">Address 1</label>                            <span class="error-Address1 text-danger"></span>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=Address2 type="text" class="form-control changable1" id="address2" placeholder="Address 2" name="Address2">                            <label for="address2">Address 2</label>                            <span class="error-Address2 text-danger"></span>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=City type="text" class="form-control changable1" id="city" placeholder="City" name="City">                            <label for="city">City</label>                            <span class="error-City text-danger"></span>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <select class="form-select changable1" aria-label="Default select example" name="state" id="cancelReason" asp-for="State">                                <option selected>Select The State</option>                                @if (Model.Regions != null)
                                {                                    @foreach (var region in Model.Regions)
                                    {                                        <option value="@region.RegionId">@region.Name</option>                                    }                                }                            </select>                            <span class="error-state text-danger"></span>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=ZipCode type="text" class="form-control changable1" id="city" placeholder="City" name="Zipcode">                            <label for="zip">ZipCode</label>                            <span class="error-Zipcode text-danger"></span>                        </div>                    </div>                    <div class="col-md-6">                        <div class="form-floating">                            <input name="PhoneNumber" type="tel" id='phone2' name="MobileNo" class="form-control changable1" placeholder="(201)555-0123" asp-for="MobileNo">                        </div>                    </div>                </div>            </form>            <h5 class="fw-bold my-3">Provider Profile</h5>            <form id="providerprofile" asp-action="Physicianprofile" asp-route-id="physicianid" enctype="multipart/form-data">                <div class="row row-cols-1 row-cols-md-2 g-3">                    <div class="col">                        <div class="form-floating">                            <input asp-for=BusinessName type="text" class="form-control changable2" id="email" placeholder="Email" name="Businessname">                            <label for="email">BusinessName</label>                        </div>                    </div>                    <div class="col">                        <div class="form-floating">                            <input asp-for=BusinessWebsite type="text" class="form-control changable2" id="email" placeholder="Email" name="Businesswebsite">                            <label for="email">BusinessWebsite</label>                        </div>                    </div>                    <div class="col">                        <div class="input-group input-group-lg">                            <div id="fileName" asp-for="FileName" class="form-control fs-6 align-middle">                                Select Photo                            </div>                            <input type="file" asp-for="File" name="photoFile" class="form-control changable2" id="fileInput" hidden accept="jpg,png" />                            <label class="input-group-text btn btn-info text-light" for="fileInput">                                <span class="d-none d-md-inline">Upload</span>                            </label>                        </div>                    </div>                    <div class="col-12 col-md-12">                        <div class="form-floating">                            <textarea class="form-control changable2" placeholder="Leave a comment here" id="floatingTextarea2" style="height: 100px" asp-for="AdminNotes"></textarea>                            <label for="floatingTextarea2">                                Admin Notes                                <span class="d-none d-sm-inline">(Optional)</span>                            </label>                        </div>                    </div>                </div>            </form>            <form class="border-top border-bottom border-secondary py-2">                <h5>Onboarding</h5>                <div class="d-flex align-items-center gap-3 mb-3 w-75 d-flex justify-content-between">                    <div class="m-0 p-0">                        <input type="checkbox" class="form-check-input" id="agreeToICA" name="agreeToICA">                        <span class="mx-5">Independent Contractor Agreement</span>                    </div>                    <div class="m-0 p-0 d-flex ">                        <input type="file" class="form-control w-75" id="ICA" hidden />                        <label class="input-group-text btn btn-info text-light" for="ICA">                            <span class="d-none d-md-inline">Upload</span>                        </label>                        <a class="btn btn-info ms-2" href="~/Physician/physicianid/ICA.pdf">View</a>                    </div>                </div>                <div class="d-flex align-items-center gap-3 mb-3 w-75 d-flex justify-content-between">                    <div class="m-0 p-0">                        <input type="checkbox" class="form-check-input" id="agreeToICA" name="agreeToICA">                        <span class="mx-5">Background Check</span>                    </div>                    <div class="m-0 p-0">                        <input type="file" class="form-control" id="Background" hidden />                        <label class="input-group-text btn btn-info text-light" for="Background">                            <span class="d-none d-md-inline">Upload</span>                        </label>                        <a class="btn btn-info" href="~/Physician/physicianid/Background.pdf">View</a>                    </div>                </div>                <div class="d-flex align-items-center gap-3 mb-3 w-75 d-flex justify-content-between">                    <div class="m-0 p-0">                        <input type="checkbox" class="form-check-input" id="agreeToICA" name="agreeToICA">                        <span class="mx-5">Hippa Compalience</span>                    </div>                    <div class="m-0 p-0">                        <input type="file" class="form-control" id="Hippa" hidden />                        <label class="input-group-text btn btn-info text-light" for="Hippa">                            <span class="d-none d-md-inline">Upload</span>                        </label>                        <a class="btn btn-info" href="~/Physician/physicianid/Hippa.pdf">View</a>                    </div>                </div>                <div class="d-flex align-items-center gap-3 mb-3 w-75 d-flex justify-content-between">                    <div class="m-0 p-0">                        <input type="checkbox" class="form-check-input" id="agreeToICA" name="agreeToICA">                        <span class="mx-5">Non-disclosure-Agreement</span>                    </div>                    <div class="m-0 p-0">                        <input type="file" class="form-control" id="NonDiscoluser" hidden />                        <label class="input-group-text btn btn-info text-light" for="NonDiscoluser">                            <span class="d-none d-md-inline">Upload</span>                        </label>                        <a class="btn btn-info" href="~/Physician/physicianid/Hippa.pdf">View</a>                    </div>                </div>                <div class="d-flex align-items-center gap-3 mb-3 w-75 d-flex justify-content-between">                    <div class="m-0 p-0">                        <input type="checkbox" class="form-check-input" id="agreeToICA" name="agreeToICA">                        <span class="mx-5">License-Documents</span>                    </div>                    <div class="m-0 p-0">                        <input type="file" class="form-control" id="License" hidden />                        <label class="input-group-text btn btn-info text-light" for="License">                            <span class="d-none d-md-inline">Upload</span>                        </label>                        <a class="btn btn-info" href="~/Physician/physicianid/License.pdf">View</a>                    </div>                </div>            </form>        </div>    </div></section>