@{
    int startDate = ViewBag.startDate.Day;
    int month = ViewBag.startDate.Month;
    int endDate = ViewBag.endDate.Day;
    int year = ViewBag.startDate.Year;
}
<form asp-action="submitTimesheetData" method="post">
    <table class="table table-borderless ">
        <thead class="text-body border-0">
            <tr>
                <th class="align-middle text-center">Date</th>
                <th class="align-middle text-center">Item</th>
                <th class="align-middle text-center">Amount</th>
                <th class="align-middle text-center" style="width: 20%">Bill</th>
                <th class="align-middle text-center">Action</th>
            </tr>
        </thead>
        <tbody>
            @{
                int j = 0;
            }
            @for (int i = startDate; i <= endDate; i++)
            {
                <tr>
                    
                    @{
                        string date = new DateTime(year, month, i).Date.ToString("dd/MM/yyyy");
                    }
                    <td class="align-middle text-center">@date</td>
                    <td class="align-middle text-center">item</td>
                    <td class="align-middle text-center">10</td>
                    <td class="align-middle text-center d-flex justify-content-center py-3" style="width: 20%">
                        <div class="input-group input-group-lg mt-5">
                            <label id="infoarea" for="fileInput" class="form-control fs-6 bg-body-secondary align-middle py-1 d-flex align-items-center">
                                Select File
                            </label>
                            <input type="file" class="form-control" id="fileInput" hidden multiple />
                            <button class="input-group-text btn btn-info text-light" id="filesubmitbtn" type="button">
                                <svg xmlns="http://www.w3.org/2000/svg"
                                     width="24"
                                     height="24"
                                     fill="currentColor"
                                     class="bi bi-cloud-arrow-up"
                                     viewBox="0 0 16 16">
                                    <path fill-rule="evenodd"
                                          d="M7.646 5.146a.5.5 0 0 1 .708 0l2 2a.5.5 0 0 1-.708.708L8.5 6.707V10.5a.5.5 0 0 1-1 0V6.707L6.354 7.854a.5.5 0 1 1-.708-.708z" />
                                    <path d="M4.406 3.342A5.53 5.53 0 0 1 8 2c2.69 0 4.923 2 5.166 4.579C14.758 6.804 16 8.137 16 9.773 16 11.569 14.502 13 12.687 13H3.781C1.708 13 0 11.366 0 9.318c0-1.763 1.266-3.223 2.942-3.593.143-.863.698-1.723 1.464-2.383m.653.757c-.757.653-1.153 1.44-1.153 2.056v.448l-.445.049C2.064 6.805 1 7.952 1 9.318 1 10.785 2.23 12 3.781 12h8.906C13.98 12 15 10.988 15 9.773c0-1.216-1.02-2.228-2.313-2.228h-.5v-.5C12.188 4.825 10.328 3 8 3a4.53 4.53 0 0 0-2.941 1.1z" />
                                </svg>
                                <span class="d-none d-md-inline">Upload</span>
                            </button>

                        </div>
                    </td>
                    <td class="align-middle text-center"></td>
                </tr>
                j++;

            }
        </tbody>
    </table>
</form>