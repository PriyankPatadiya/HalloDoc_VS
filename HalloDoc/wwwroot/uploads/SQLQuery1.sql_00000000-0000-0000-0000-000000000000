/*SELECT o.ord_no, o.purch_amt, c.cust_name, c.city from ordered as o Inner join customer as c on o.customer_id = c.customer_id where o.purch_amt between 500 and 2000;*/
SELECT c.cust_name , c.city, s.name , s.commission from customer as c Inner join salesman as s on s.salesman_id = c.salesman_id;